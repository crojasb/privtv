{
  "name": "privtv",
  "version": "0.0.1",
  "description": "Alquiler de peliculas en streaming por suscripcion mensual",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --platform win32 --arch all --out build/ --overwrite --icon=./icon.ico",
    "setup": "electron-installer-windows --src build/privtv-win32-x64/ --dest build/installers/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "peliculas",
    "suscripcion",
    "streaming"
  ],
  "author": "Priv.TV",
  "license": "ISC",
  "devDependencies": {
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "7.0.1",
    "electron-prebuilt": "1.0.1"
  },
  "dependencies": {
    "request": "^2.72.0",
    "srt-to-vtt": "^1.1.1",
    "videogular-buffering": "^1.4.1"
  }
}
